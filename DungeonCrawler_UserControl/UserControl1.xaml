        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="530" VerticalAlignment="Top" Width="1000" KeyDown="canvas_KeyDown" Focusable="True">
            <Canvas.Background>
                <ImageBrush ImageSource="d9nay7u-51e0195c-0077-46b9-8753-d7222888ab77.png"/>
